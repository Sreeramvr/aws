<script type="text/javascript">


function downloadJSAtOnload() {
	 var ran=Math.random();
	var modules = [
	{ include: true, incfile:'js/MainPageNew.js?r='+ran }, 
	{ include: true, incfile:'js/popup_new.js' } ,
	{ include: true, incfile:'js/base_file/mask/ui.mask.js' } ,  
	{ include: true, incfile:'js/base_file/mask/ui.core.js' } , 
	{ include: true, incfile:'js/MultiGrid.js' } , 
	{ include: true, incfile:'js/approvals.js' } , 
	{ include: true, incfile:'js/manage_menu.js' } , 
	{ include: true, incfile:'js/right_panel.js' } , 
	{ include: true, incfile:'assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js' },  
	{ include: true, incfile:'assets/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js' },
	{ include: true, incfile:'plugins/bower_components/clockpicker/dist/jquery-clockpicker.min.js' },
	{ include: true, incfile:'custom_js/month_year.js'} , 
	{ include: true, incfile:'custom_js/custom_dashboard_functions.js' },
	{ include: true, incfile:'custom_js/PreMultipleSaveValidation.js' },
	];
	var filename;
		for(var i=0;i<modules.length; i++)
		{
			if(modules[i].include === true) {        	
				filename =  modules[i].incfile;
				var element = document.createElement("script");
				element.src =filename;
				document.body.appendChild(element);
				<!-- console.log(i); -->
			}
		}
}
if (window.addEventListener)
window.addEventListener("load", downloadJSAtOnload, false);
else if (window.attachEvent)
window.attachEvent("onload", downloadJSAtOnload);
else window.onload = downloadJSAtOnload;
