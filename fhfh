 <script>
 $("#ref_display_popup").popup({
 escape: "true" ,
   opacity: 0.6,
	offsettop:60,   color:'#000',
   transition: "all 0.3s",  autozindex: true
 });
 
function changeToSmallCSS() {
    $('link[href="css/tarka_font_small.css"]')[0].disabled=false;
    $('link[href="css/tarka_font_medium.css"]')[0].disabled=true;
    $('link[href="css/tarka_font_large.css"]')[0].disabled=true;
	$("#small_font").addClass('current_font');
	$("#medium_font").removeClass('current_font');
	$("#large_font").removeClass('current_font');
	setTimeout(function(){ HandleWindowResize();}, 200);
}
function changeToMediumCSS() {
    $('link[href="css/tarka_font_small.css"]')[0].disabled=true;
    $('link[href="css/tarka_font_medium.css"]')[0].disabled=false;
    $('link[href="css/tarka_font_large.css"]')[0].disabled=true;
	$("#small_font").removeClass('current_font');
	$("#medium_font").addClass('current_font');
	$("#large_font").removeClass('current_font');
	setTimeout(function(){ HandleWindowResize();}, 200);
}
function changeToLargeCSS() {
    $('link[href="css/tarka_font_small.css"]')[0].disabled=true;
    $('link[href="css/tarka_font_medium.css"]')[0].disabled=true;
    $('link[href="css/tarka_font_large.css"]')[0].disabled=false;
	$("#small_font").removeClass('current_font');
	$("#medium_font").removeClass('current_font');
	$("#large_font").addClass('current_font');
	setTimeout(function(){ HandleWindowResize();}, 200);
}

$(window).on('resize', function() {
	HandleWindowResize();
});

</script> 
</body>

</html>